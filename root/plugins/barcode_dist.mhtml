<%args>
$object
</%args>

<%doc>
This component creates a chart showing which barcodes were returned from a
sequencing run.
</%doc>

% if ($object->multiplexing_type()->name() eq 'multiplexed') {
<div class="sect">
<span class="sect-title disclosed-title">
  <a href="#">Barcode distribution</a>
</span>
<div class="sect-content">
  <img src='<% $c->uri_for("/plugin/graph/barcode_dist", $object->sequencingrun_id()) %>'/>
</div>
</div>
% }
